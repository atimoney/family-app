{
  "name": "@family/web",
  "author": "Aaron Timoney",
  "version": "1.1.1",
  "description": "Vite Starter & TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "pnpm lint:fix && pnpm fm:fix",
    "clean": "rm -rf node_modules dist build out .vite",
    "re:dev": "pnpm clean && pnpm install && pnpm dev",
    "re:build": "pnpm clean && pnpm install && pnpm build",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "tsc --showConfig"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@family/shared": "workspace:*",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/nunito-sans": "^5.2.7",
    "@fontsource-variable/public-sans": "^5.2.7",
    "@fontsource/barlow": "^5.2.8",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@hookform/resolvers": "^5.2.2",
    "@iconify/react": "^6.0.2",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@mui/stylis-plugin-rtl": "^7.3.6",
    "@mui/x-data-grid": "^8.22.0",
    "@mui/x-date-pickers": "^8.22.0",
    "@mui/x-tree-view": "^8.22.0",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "es-toolkit": "^1.43.0",
    "framer-motion": "^12.23.26",
    "minimal-shared": "^1.1.5",
    "nprogress": "^0.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.68.0",
    "react-router": "^7.10.1",
    "simplebar-react": "^3.3.2",
    "stylis": "^4.3.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/node": "^25.0.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/stylis": "^4.2.7",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-checker": "^0.12.0"
  }
}
